                }
            }
            unset($scripts, $script);
            break;
        case 'check':
            // os & kernel
            $bit = '('.trim(sysCmd('getconf LONG_BIT')[0]).'bit)';
            $release = trim(sysCmd('uname -sr')[0]);
            $machine = trim(sysCmd('uname -m')[0]);
            if (strpos(' '.strtolower($release), 'arch')) {
                $os = 'ARCH';
            } else {
                $os = 'RPiOS';
            }
            $redis->exists('os') || $redis->set('os', $os);
            $redis->exists('kernel') || $redis->set('kernel', $release.' '.$os.' '.$machine.' '.$bit);
            $redis->exists('i2smodule') || $redis->set('i2smodule', 'none');
