            $redis->hExists('addons', 'font') || $redis->hSet('addons', 'font', '20220901');
            break;
    }
}
// inspect console command
if (isset($argv[1])) {
    switch ($argv[1]) {
        case 'reset':
            redisDatastore($redis,'mpdreset');
            redisDatastore($redis,'apreset');
            redisDatastore($redis,'reset');
            sysCmd('/srv/http/db/redis_acards_details');
            wrk_mpdconf($redis, 'writecfg');
            break;
        case 'check':
            redisDatastore($redis,'mpdcheck');
            redisDatastore($redis,'apcheck');
            redisDatastore($redis,'check');
            redisDatastore($redis,'opcachecheck');
            break;
        case 'mpdreset':
            redisDatastore($redis,'mpdreset');
            sysCmd('/srv/http/db/redis_acards_details');
            break;
        case 'mpdcheck':
            redisDatastore($redis,'mpdcheck');
            break;
        case 'apreset':
            redisDatastore($redis,'apreset');
            break;
        case 'apcheck':
            redisDatastore($redis,'apcheck');
            break;
        case 'opcachecheck':
            redisDatastore($redis,'opcachecheck');
            break;
        default:
            trigger_error("RuneAudio Datastore reset\nredis_datastore_setup {reset|check|mpdreset|mpdcheck|apreset|apcheck|opcachecheck}\n", E_USER_ERROR);
            break;
    }
} else {
    trigger_error("RuneAudio Datastore reset\nredis_datastore_setup {reset|check|mpdreset|mpdcheck|apreset|apcheck|opcachecheck}\n", E_USER_ERROR);
}
