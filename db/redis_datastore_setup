        case 'opcachecheck':
            // all RuneAudio php scripts
            $scripts = sysCmd('grep -Ril "<?php" /srv 2> /dev/null | grep -vi "vendor" | grep -v ":" | grep -vi git | grep -vi .zip | grep -vi .phar');
            foreach ($scripts as $script) {
                if (!$redis->exists('php_opcache_prime', $script)) {
